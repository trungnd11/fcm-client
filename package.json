{
  "name": "@f88/fcm-notification",
  "version": "1.0.11",
  "type": "module",
  "bin": {
    "fcm-notification": "./scripts/cli.js"
  },
  "files": [
    "dist",
    "public",
    "scripts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rslib build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:watch": "concurrently \"rslib build --watch\" \"vue-tsc --watch --declaration --emitDeclarationOnly --outDir dist\"",
    "build:storybook": "storybook build",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "storybook": "storybook dev",
    "test": "vitest run",
    "fcm:generate": "node scripts/fcm-worker.js generate",
    "fcm:validate": "node scripts/fcm-worker.js validate",
    "fcm:config": "node scripts/fcm-worker.js update-config",
    "fcm:init": "node scripts/cli.js init-worker",
    "fcm:setup": "node scripts/setup-cli.js",
    "publish-lib": "pnpm publish --no-git-checks"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@eslint/js": "^9.30.0",
    "@rsbuild/core": "~1.5.4",
    "@rsbuild/plugin-vue": "^1.1.2",
    "@rslib/core": "^0.13.0",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-essentials": "^9.0.0-alpha.12",
    "@storybook/addon-interactions": "^9.0.0-alpha.10",
    "@storybook/addon-links": "^9.0.17",
    "@storybook/addon-onboarding": "^9.0.17",
    "@storybook/blocks": "^9.0.0-alpha.17",
    "@storybook/test": "^9.0.0-alpha.2",
    "@storybook/vue3": "^9.0.17",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.30.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-eslint": "^16.4.2",
    "rsbuild-plugin-dts": "^0.13.0",
    "rsbuild-plugin-unplugin-vue": "^0.1.0",
    "storybook": "^9.0.17",
    "storybook-addon-rslib": "^2.1.0",
    "storybook-builder-rsbuild": "^2.1.0",
    "storybook-vue3-rsbuild": "^2.1.0",
    "tailwindcss": "^4.1.12",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.3"
  },
  "peerDependencies": {
    "vue": "3.5.18"
  },
  "private": false,
  "dependencies": {
    "@ant-design/icons-vue": "7.0.1",
    "ant-design-vue": "4.2.6",
    "axios": "^1.11.0",
    "dotenv": "^17.2.2",
    "firebase": "^12.0.0",
    "jwt-decode": "^4.0.0"
  },
  "pnpm": {
    "overrides": {
      "@rsbuild/core": "1.5.4"
    }
  }
}
